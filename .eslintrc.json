{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/extensions": "off",
        "no-named-as-default-member": "off",
        "no-console": "off",
        "no-extraneous-dependencies": "off"
    }
}
